<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>jersey-serlvet</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
      <param-name>jersey.config.server.provider.packages</param-name>
      <param-value>com.minal.scheduler.api</param-value>
    </init-param>
    <init-param>
      <param-name>jersey.config.server.provider.classnames</param-name>
      <param-value>org.glassfish.jersey.media.multipart.MultiPartFeature</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>jersey-serlvet</servlet-name>
    <url-pattern>/api/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>LoginController</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.minal.scheduler.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>CreateEventController</display-name>
    <servlet-name>CreateEvent</servlet-name>
    <servlet-class>com.minal.scheduler.controller.CreateEventController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateEvent</servlet-name>
    <url-pattern>/createevent</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>SelectInterviewersController</display-name>
    <servlet-name>SelectInterviewers</servlet-name>
    <servlet-class>com.minal.scheduler.controller.SelectInterviewersController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelectInterviewers</servlet-name>
    <url-pattern>/selectint</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>HomepageRecruiterController</display-name>
    <servlet-name>HomepageRecruiter</servlet-name>
    <servlet-class>com.minal.scheduler.controller.HomepageRecruiterController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomepageRecruiter</servlet-name>
    <url-pattern>/homepagerecruiter</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>HomepageInterviewerController</display-name>
    <servlet-name>HomepageInterviewer</servlet-name>
    <servlet-class>com.minal.scheduler.controller.HomepageInterviewerController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomepageInterviewer</servlet-name>
    <url-pattern>/homepageinterviewer</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>InterviewRequestController</display-name>
    <servlet-name>InterviewRequest</servlet-name>
    <servlet-class>com.minal.scheduler.controller.InterviewRequestController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InterviewRequest</servlet-name>
    <url-pattern>/interviewrequest</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>ResponseController</display-name>
    <servlet-name>Response</servlet-name>
    <servlet-class>com.minal.scheduler.controller.ResponseController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Response</servlet-name>
    <url-pattern>/response</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>EventDetailsForInterviewerController</display-name>
    <servlet-name>EventDetailsForInterviewer</servlet-name>
    <servlet-class>com.minal.scheduler.controller.EventDetailsForInterviewerController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventDetailsForInterviewer</servlet-name>
    <url-pattern>/eventdetailsforinterviewer</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>EventDetailsForRecruiterController</display-name>
    <servlet-name>EventDetailsForRecruiter</servlet-name>
    <servlet-class>com.minal.scheduler.controller.EventDetailsForRecruiterController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventDetailsForRecruiter</servlet-name>
    <url-pattern>/eventdetailsforrecruiter</url-pattern>
  </servlet-mapping>
</web-app>
